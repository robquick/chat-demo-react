{
    "firebase": "brownbag-hsg",
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
        {
            "source": "/public/main.js",
            "destination": "/main.js"
        },
        {
            "source": "**",
            "destination": "/index.html"
        }
    ]
}
